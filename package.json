{
  "name": "create-fastify-app",
  "version": "1.5.6",
  "description": "A Fastify application generator",
  "main": "create-fastify-app.js",
  "bin": {
    "create-fastify-app": "create-fastify-app.js"
  },
  "scripts": {
    "lint": "standard --verbose",
    "unit": "tap test/create*.test.js && tap test/generate.project.test.js && tap test/generate.project.swagger.test.js && tap test/start.project.cors.test.js && tap test/start.project.mysql.test.js && tap test/start.project.mongo.test.js && tap test/start.project.postgres.test.js && tap test/start.project.redis.test.js && tap test/start.project.service.test.js && tap test/start.project.test.js",
    "coverage": "tap test/*.test.js --coverage && tap --coverage-report=text-lcov | coveralls",
    "test": "npm run lint && npm run unit"
  },
  "keywords": [
    "fastify",
    "fastify-swagger",
    "fastify-plugin",
    "nodejs",
    "nodejs-library",
    "redis",
    "cors",
    "mongodb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidedantonio/fastify-generator.git"
  },
  "author": "Davide D'Antonio <davide.dantonio@webeetle.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidedantonio/fastify-generator/issues"
  },
  "homepage": "https://github.com/davidedantonio/fastify-generator#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "codecov": "^3.2.0",
    "command-exists": "^1.2.8",
    "commist": "^1.0.0",
    "fastify": "^2.0.1",
    "fastify-plugin": "^1.5.0",
    "generify": "^3.0.2",
    "handlebars": "^4.1.2",
    "inquirer": "^6.2.2",
    "js-beautify": "^1.8.9",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^6.1.0",
    "lodash": "^4.17.11",
    "make-promises-safe": "^5.0.0",
    "swagger-parser": "^6.0.5",
    "yargs-parser": "^13.0.0"
  },
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "coveralls": "^3.0.3",
    "dotenv": "^7.0.0",
    "fastify-autoload": "^0.8.0",
    "fastify-cors": "^2.1.1",
    "fastify-mongodb": "^1.0.1",
    "fastify-mysql": "^0.2.0",
    "fastify-postgres": "^2.0.0",
    "fastify-redis": "^3.0.0",
    "fastify-swagger": "^2.3.0",
    "is-docker": "^2.0.0",
    "mkdirp": "^0.5.1",
    "pg": "^7.9.0",
    "pino-colada": "^1.4.4",
    "pump": "^3.0.0",
    "resolve-from": "^5.0.0",
    "rimraf": "^2.6.3",
    "simple-get": "^3.0.3",
    "standard": "^12.0.1",
    "tap": "13.1.0",
    "walker": "^1.0.7"
  },
  "greenkeeper": {
    "ignore": [
      "request"
    ]
  }
}
